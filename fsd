  <!-- Simple Portfolio Page -->
  <h1>Onkar Jituri</h1>
  <h2>Student | MCA (Sem-1)</h2>

  <h3>About Me</h3>
  <p>I enjoy learning web development and creating simple projects.</p>

  <h3>Skills</h3>
  <ul>
    <li>HTML & CSS</li>
    <li>JavaScript</li>
    <li>React (Beginner)</li>
  </ul>

  <h3>Contact</h3>
  <p>Email: onkar@example.com</p>

  <p>Portfolio created by Onkar Jituri.</p>

  <!-- Survey Form -->
  <h1>Student Survey Form</h1>
  <form>
    <label>Name:</label>
    <input type="text" name="name" required><br><br>

    <label>Age:</label>
    <input type="number" name="age" min="1" max="100" required><br><br>

    <label>Gender:</label>
    <input type="radio" name="gender" value="Male"> Male
    <input type="radio" name="gender" value="Female"> Female
    <input type="radio" name="gender" value="Other"> Other
    <br><br>

    <label>Favorite Subject:</label>
    <input type="text" name="subject"><br><br>

    <button type="submit">Submit</button>
  </form>

  <p>Form created by Onkar Jituri.</p>
</body>
</html>
